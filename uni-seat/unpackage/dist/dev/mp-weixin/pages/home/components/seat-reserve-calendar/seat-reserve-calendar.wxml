<view class="col seat-calendar"><view class="row top"><button data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="btn cancel" bindtap="__e">{{$root.m0}}</button><text class="title">{{$root.m1}}</text><button data-event-opts="{{[['tap',[['certain',['$event']]]]]}}" class="btn certain" bindtap="__e">{{$root.m2}}</button></view><view class="weekdays-view"><block wx:for="{{weakDays}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="week-day"><view class="week-day-block">{{''+item+''}}</view></view></block></view><view class="day-list"><block wx:for="{{30}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['didSelectorDate',['$0'],[[[30,'',index]]]]]]]}}" class="day" bindtap="__e"><view class="day-block"><view class="{{['left',dayBlockClassName]}}"></view><view class="{{['right',dayBlockClassName]}}"></view><text style="z-index:999;">{{item}}</text></view></view></block></view><view class="row bottom-view"><button data-event-opts="{{[['tap',[['didBottomBtnAction',[1]]]]]}}" class="btn morning" bindtap="__e">{{$root.m3}}</button><button data-event-opts="{{[['tap',[['didBottomBtnAction',[2]]]]]}}" class="btn noon" bindtap="__e">{{$root.m4}}</button><button data-event-opts="{{[['tap',[['didBottomBtnAction',[3]]]]]}}" class="btn day" bindtap="__e">{{$root.m5}}</button></view></view>