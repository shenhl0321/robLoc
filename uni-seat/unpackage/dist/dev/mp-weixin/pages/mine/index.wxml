<view class="main-view" style="background-color:#F5F5F5;"><view class="col top-view"><image class="bg top-bg" src="/static/my_top_bg.png" mode="aspectFill"></image><view class="nav-bar"><text class="title">{{$root.m0}}</text></view><view class="col user"><view class="col content"><image class="user-header" src="{{avatarUrl}}" mode="aspectFill"></image><text class="user-name">{{realname}}</text><text class="mobile">{{phone}}</text><image class="set" src="/static/my_set.png" data-event-opts="{{[['tap',[['pushToSetPage',['$event']]]]]}}" bindtap="__e"></image></view></view></view><view class="col section-list"><view><block wx:for="{{sectionList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['didClickSection',['$0'],[[['sectionList','',index]]]]]]]}}" class="row my-section" bindtap="__e"><view class="row"><image class="icon" src="{{item.src}}"></image><text class="name">{{item.name}}</text></view><image class="small_push" src="/static/ic_push_small.png"></image></view></block></view><button data-event-opts="{{[['tap',[['exitBtnAction',['$event']]]]]}}" class="exit-btn" bindtap="__e">{{$root.m1}}</button></view></view>